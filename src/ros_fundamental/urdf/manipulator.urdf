<?xml version="1.0"?>
<robot name="manipulator">

  <!-- Link definitions -->
  <link name="base_link">
  <visual>
      <geometry>
          <cylinder radius="0.05" length="0.1"/>
      </geometry>
      <material name="gray">
        <color rgba=".2 .2 .2 1" />
      </material>
    </visual>

    <inertial>
        <mass value="1" />
        <inertia ixx="0.01" ixy="0.0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <collision>
      <geometry>
          <cylinder radius="0.05"  length="0.1"/>
      </geometry>
    </collision>
  </link>


  <link name="frame_down_link">
   <visual>
      <geometry>
          <cylinder radius="0.05" length="0.5"/>
      </geometry>
      <material name="gray">
        <color rgba=".2 .2 .2 1" />
      </material>
    </visual>

    <inertial>
        <mass value="1" />
        <inertia ixx="0.01" ixy="0.0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <collision>
      <geometry>
          <cylinder radius="0.05"  length="0.5"/>
      </geometry>
    </collision>
  </link>

  <link name="frame_middle_link">
   <visual>
      <geometry>
          <cylinder radius="0.05" length="0.5"/>
      </geometry>
      <material name="gray">
        <color rgba=".2 .2 .2 1" />
      </material>
    </visual>

    <inertial>
        <mass value="1" />
        <inertia ixx="0.01" ixy="0.0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <collision>
      <geometry>
          <cylinder radius="0.05"  length="0.5"/>
      </geometry>
    </collision>
  </link>

  <link name="frame_up_link">
   <visual>
      <geometry>
          <cylinder radius="0.05" length="0.5"/>
      </geometry>
      <material name="gray">
        <color rgba=".2 .2 .2 1" />
      </material>
    </visual>

    <inertial>
        <mass value="1" />
        <inertia ixx="0.01" ixy="0.0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <collision>
      <geometry>
          <cylinder radius="0.05"  length="0.5"/>
      </geometry>
    </collision>
  </link>

  <link name="finger_link">
   <visual>
      <geometry>
          <cylinder radius="0.05" length="0.3"/>
      </geometry>
      <material name="gray">
        <color rgba=".2 .2 .2 1" />
      </material>
    </visual>

    <inertial>
        <mass value="1" />
        <inertia ixx="0.01" ixy="0.0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <collision>
      <geometry>
          <cylinder radius="0.05"  length="0.3"/>
      </geometry>
    </collision>
  </link>


  


  <joint name="frame_base_joint" type="continuous">
    <parent link="base_link"/>
    <child link="frame_down_link"/>
    <origin xyz="0.0 0.0 0.25" rpy="0.0 0.0 0.0"/>
    <axis xyz="0.0 0.0 1.0"/>
  </joint>



  <joint name="frame_down_joint" type="revolute">
    <parent link="frame_down_link"/>
    <child link="frame_middle_link"/>
    <origin xyz="0.1 0.0 0.25" rpy="1.57 0.0 0.0"/>
    <axis xyz="1.0 0.0 0.0"/>
    <limit effort="10" velocity="1.0" lower="-0.5" upper="0.7" />
  </joint>

  <joint name="frame_middle_joint" type="revolute">
    <parent link="frame_middle_link"/>
    <child link="frame_up_link"/>
    <origin xyz="0.1 0.0 0.25" rpy="1.57 0.0 0.0"/>
    <axis xyz="1.0 0.0 0.0"/>
    <limit effort="10" velocity="1.0" lower="-0.5" upper="0.7" />
  </joint>

   <joint name="finger_joint" type="prismatic">
    <parent link="frame_up_link"/>
    <child link="finger_link"/>
    <origin xyz="0.0 0.0 0.30" rpy="1.57 0.0 0.0"/>
    <axis xyz="0.0 0.0 1.0"/>
    <limit effort="10" velocity="1.0" lower="-0.2" upper="0.2" />
  </joint>

 

</robot>
